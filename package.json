{
  "project": "MVC-Boilerplate",
  "name": "goodboy",
  "version": "0.0.1",
  "description": "A simple boilerplate for front-end Development.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode=development",
    "start:dev": "webpack-dev-server --open --mode=development",
    "start:prod": "webpack-dev-server --open --mode=production",
    "build": "webpack --mode=development",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "build-stats": "webpack --stats=minimal --mode=production --profile --json > stats.json",
    "analyze-stats": "webpack-bundle-analyzer stats.json",
    "analyze": "npm-run-all build-stats analyze-stats"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "boilerplate",
    "javascript",
    "es6",
    "webpack"
  ],
  "author": "Aykut Sezgin",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "require-context": "^1.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^6.0.11",
    "svg-spritemap-webpack-plugin": "^3.3.1",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.8.2",
    "bootstrap": "^4.2.1",
    "detect-browser": "^4.1.0",
    "jquery": "^3.3.1",
    "multi-clamp": "^1.1.0",
    "popper.js": "^1.14.7",
    "slick-carousel": "^1.8.1",
    "throttle-debounce": "^2.1.0"
  }
}
